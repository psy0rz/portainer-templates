                {
                    "name": "OLS_VERSION",
                    "label": "Openlitespeed docker image version (1.7.11-lsphp73, 1.8.1-lsphp74, 1.8.1-lsphp80, 1.8.3lsphp81, 1.8.3-lsphp82, 1.8.3-lsphp83, 1.8.3-lsphp84, latest)",
                    "default": "1.8.3-lsphp84"
